{
    "name": "relewise/client",
    "description": "Relewise is a next generation personalization SaaS-platform, which offers functionality within product- and content recommendations and personalized search. This official SDK helps you interact with our API.",
    "type": "library",
    "version": "1.0.0",
    "keywords": [
        "relewise",
    	"recommendations",
    	"personalization"
    ],
    "homepage": "https://relewise.com",
    "license": "MIT",
    "config": {
        "platform": {
            "php": "8.1"
        },
        "sort-packages": true,
        "process-timeout": 7200,
        "allow-plugins": {
            "fqqdk/packagist-publisher": true
        }
    },
    "autoload": {
		"psr-4": {
			"Relewise\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Relewise\\Tests\\": "tests"
		}
	},
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "9.5.28",
        "fqqdk/packagist-publisher": "*"
    },
    "scripts": {
    }
}
